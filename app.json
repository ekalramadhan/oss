{
  "expo": {
    "name": "Operations-Support-Systems",
    "slug": "Operations-Support-Systems",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./source/images/iconfix.png",
    "splash": {
      "image": "./source/images/splashfix.png",
      "resizeMode": "contain",
      "backgroundColor": "#002171"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"] 
    }
  }
}
